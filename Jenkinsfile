pipeline {
    agent any

    stages {
        stage('Compile') {
            steps {
                echo 'COMEPILE-Hello World'
            }
        }
         stage('Test') {
            steps {
                echo 'TEST-Hello World'
            }
        }
        stage('Package') {
            steps {
                echo 'PACKAGE-Hello World'
            }
        }
    }
}
