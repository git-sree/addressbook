    agent any

    stages {
        stage('compile') {
            steps {
                scripts {
                    echo "compile- hello world"
                }
            }
        }

        stage('test') {
            steps {
                scripts {
                    echo "test- hello world"
                }
            }
        }

        stage('package') {
            steps {
                scripts {
                    echo "package- hello world"
                }
            }
        }
    }
}
